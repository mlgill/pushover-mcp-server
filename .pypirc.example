# PyPI configuration template
# Copy to ~/.pypirc and fill in your token

[distutils]
index-servers =
    pypi
    testpypi

# Public PyPI (if you ever want to publish publicly)
[pypi]
repository = https://upload.pypi.org/legacy/
username = __token__
password = pypi-YOUR_API_TOKEN_HERE

# Test PyPI (for testing before public release)
[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = pypi-YOUR_TEST_API_TOKEN_HERE
